{
  "expo": {
    "name": "Dishcovery",
    "slug": "dishcovery",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "dishcovery",
    "plugins": ["expo-router"],
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dishcovery.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dishcovery.app"
    },
    "web": {
      "favicon": "./src/assets/favicon.png",
      "bundler": "metro",
      "name": "Dishcovery",
      "shortName": "Dishcovery",
      "description": "AI-powered recipe generator from food photos",
      "themeColor": "#1F4E79",
      "backgroundColor": "#F6F4F4"
    },
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}
